{
  "requireSudo": true,
  "onStart": [
    "if [ \"$PROFILE\" = \"coop\" ]; then JAR_WORKDIR=\"$HOME/.config/NinjaLink\" $SCRIPT_DIR/run_jar.sh NinjaLink; fi",
    "$SCRIPT_DIR/run_jar.sh Ninjabrain-Bot",
    "flatpak run com.obsproject.Studio",
    "sudo input-remapper-control --command start --device \"Ducky Ducky One 3 TKL \" --preset \"MCSR\"",
    "input-remapper-control --command start --device \"Razer Razer Viper V3 Pro\" --preset \"MCSR\"",
    "# $SCRIPT_DIR/run_jar.sh paceman",
    "if [ \"$PROFILE\" = \"ranked\" ]; then prismlauncher -l \"ranked\"; fi",
    "if [ -z \"${PROFILE:-}\" ]; then prismlauncher -l \"1.16.1\"; fi"
  ],
  "onDestroy": [
    "sudo input-remapper-control --command stop-all"
  ],
  "onToggleBinds": [
    "# notify-send \"Binds toggled: $BINDS_ENABLED\""
  ],
  "binds": {
    "toggleBinds": ",HOME",
    "modeSwitch": {
      "tall": ",H",
      "boat-eye": ",J",
      "planar-abuse": ",Z"
    },
    "custom": {}
  },
  "modeSwitch": {
    "default": {
      "size": "1920x1080",
      "sensitivity": "0.0",
      "onExit": [
        "# notify-send Leaving $PREVIOUS_MODE"
      ],
      "onEnter": [
        "# notify-send Entering $NEXT_MODE"
      ]
    },
    "modes": {
      "normal": {},
      "tall": {
        "size": "350x750"
      },
      "boat-eye": {
        "size": "384x16384",
        "sensitivity": "-0.9375",
        "onEnter": [
          "# ~/.local/bin/razer-cli --dpi 100",
          "# ~/.local/bin/obs-cli item show --scene BoatEyeScene GameCapture"
        ],
        "onExit": [
          "# ~/.local/bin/razer-cli --dpi 1800",
          "# ~/.local/bin/obs-cli item hide --scene BoatEyeScene GameCapture"
        ]
      },
      "planar-abuse": {
        "size": "1920x420"
      }
    }
  },
  "minecraft": {
    "onStart": [],
    "prismPrefixOverride": "~/.local/share/PrismLauncher",
    "windowClassRegex": "Minecraft",
    "windowTitleRegex": "Minecraft",
    "prismWrapperCommand": {
      "autoReplace": true,
      "innerCommand": "obs-gamecapture",
      "prismMinecraftInstanceIds": [
        "1.16.1"
      ]
    },
    "observeLog": {
      "enabled": true
    }
  },
  "autoDestroyOnExit": true
}
