{
  "binds": {
    "toggleBinds": ",HOME",
    "modeSwitch": {
      "tall": ",H",
      "boat-eye": ",J",
      "planar-abuse": ",Z"
    }
  },
  "modeSwitch": {
    "default": {
      "size": "1920x1080",
      "sensitivity": "0.0",
      "onExit": "# notify-send Leaving $PREVIOUS_MODE",
      "onEnter": "# notify-send Entering $NEXT_MODE"
    },
    "modes": {
      "normal": {},
      "tall": {
        "size": "350x750"
      },
      "boat-eye": {
        "size": "384x16384",
        "sensitivity": "-0.9375",
        "onEnter": "# razer-cli --dpi 100; obs-cli sceneitem show BoatEyeScene GameCapture",
        "onExit": "# razer-cli --dpi 1800; obs-cli sceneitem hide BoatEyeScene GameCapture"
      },
      "planar-abuse": {
        "size": "1920x420"
      }
    }
  },
  "inputRemapper": {
    "devices": [
      {
        "device": "Razer Razer Viper V3 Pro",
        "preset": "MCSR"
      },
      {
        "device": "Ducky Ducky One 3 TKL ",
        "preset": "MCSR"
      }
    ]
  },
  "minecraft": {
    "prismPrefixOverride": "~/.local/share/PrismLauncher",
    "prismInstanceId": "1.16.1",
    "windowTitleRegex": "^Minecraft\\* 1.16.1",
    "observeLog": {
      "enabled": true
    }
  },
  "pipewireLoopback": {
    "enabled": false
  },
  "autoDestroyOnExit": true
}
